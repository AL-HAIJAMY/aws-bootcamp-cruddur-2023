#! /usr/bin/bash

docker build \
--build-arg REACT_APP_BACKEND_URL="https://api.alhaijamy.com" \
--build-arg REACT_APP_AWS_PROJECT_REGION="$AWS_DEFAULT_REGION" \
--build-arg REACT_APP_AWS_COGNITO_REGION="$AWS_DEFAULT_REGION" \
--build-arg REACT_APP_AWS_USER_POOLS_ID="us-east-1_OwzvQ0O4r" \
--build-arg REACT_APP_CLIENT_ID="17g2kmma01nfb2vf2vmk79e7dt" \
-t frontend-react-js \
-f Dockerfile.prod \
.